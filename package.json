    "babel-preset-es2015": "^6.9.0",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "elliptical": "^1.0.0-rc16",
    "lacona-api": "^0.6.2",
    "lacona-phrases": "^0.8.0",
    "moment": "^2.13.0"
